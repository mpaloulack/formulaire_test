{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [
            false
        ],
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "member-ordering": false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "*.json",
            "**/*.json"
        ]
    }
}